{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "@openai_api_key",
    "YOUTUBE_API_KEY": "@youtube_api_key",
    "QNA_APP_TOKEN": "@qna_app_token"
  }
} 